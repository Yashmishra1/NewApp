{
  "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
      "baseUrl": "src",
      "module": "commonjs",
      "target": "es6",
      "paths": {
        "@/*": [ "./*" ],
        "@assets": [ "./assets" ],
        "@assets/*": [ "./assets/*" ],
        "@components": [ "./components" ],
        "@components/*": [ "./components/*" ],
        "@constants": [ "./constants" ],
        "@constants/*": [ "./constants/*" ],
        "@navigation": [ "./navigation" ],
        "@navigation/*": [ "./navigation/*" ],
        "@redux": [ "./redux" ],
        "@redux/*": [ "./redux/*" ],
        "@store": [ "./redux/store" ],
        "@store/*": [ "./redux/store/*" ],
        "@screens": [ "./screens" ],
        "@screens/*": [ "./screens/*" ],
        "@services": [ "./services" ],
        "@services/*": [ "./services/*" ],
        "@utils": [ "./utils"],
        "@utils/*": [ "./utils/*"],
        "@theme": [ "./theme" ],
        "@theme/*": [ "./theme/*" ],
        "@thirdParty": [ "./services/thirdParty"],
        "@thirdParty/*": [ "./services/thirdParty/*"],
        "@mock": [ "./utils/mock" ],
        "@mock/*": [ "./utils/mock/*" ],
      },
    },
    "exclude": [
      "node_modules",
      "**/node_modules/*"
    ]
}